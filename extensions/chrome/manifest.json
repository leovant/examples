{
  "name": "br.net.cartoriodigital",
  "version": "1.0.0",
  "description": "Cart√≥rio Digital",
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "all_frames": true,
      "js": ["content.js"],
      "matches": ["https://*.cartoriodigital.net.br/*"]
    }
  ],
  "externally_connectable": {
    "matches": ["https://*.cartoriodigital.net.br/*"]
  },
  "permissions": [
    "activeTab",
    "declarativeContent",
    "downloads",
    "nativeMessaging",
    "storage"
  ],
  "page_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/get_started16.png",
      "32": "images/get_started32.png",
      "48": "images/get_started48.png",
      "128": "images/get_started128.png"
    }
  },
  "options_page": "options.html",
  "icons": {
    "16": "images/get_started16.png",
    "32": "images/get_started32.png",
    "48": "images/get_started48.png",
    "128": "images/get_started128.png"
  }
}
